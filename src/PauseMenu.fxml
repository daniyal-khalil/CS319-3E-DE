<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane nodeOrientation="RIGHT_TO_LEFT" prefHeight="257.0" prefWidth="423.0" style="-fx-background-color: #E5E8E8;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" ellipsisString="" layoutX="110.0" layoutY="14.0" prefHeight="53.0" prefWidth="201.0" text="Pause" textOverrun="CENTER_WORD_ELLIPSIS" wrapText="true">
         <font>
            <Font name="Haettenschweiler" size="48.0" />
         </font>
      </Label>
      <Button layoutX="32.0" layoutY="91.0" mnemonicParsing="false" prefHeight="55.0" prefWidth="360.0" text="Continue" fx:id="continueGame" onAction="#continueGame">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="31.0" layoutY="162.0" mnemonicParsing="false" prefHeight="55.0" prefWidth="360.0" text="End game and exit" fx:id="exitGame" onAction="#exittingGame">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</Pane>
