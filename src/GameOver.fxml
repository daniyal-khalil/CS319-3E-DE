<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane nodeOrientation="RIGHT_TO_LEFT" prefHeight="257.0" prefWidth="423.0" style="-fx-background-color: #E5E8E8;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
      <ImageView fitHeight="462.0" fitWidth="885.0" layoutX="-1.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/gameplay.jpg" />
         </image>
      </ImageView>
        <Label alignment="CENTER" contentDisplay="CENTER" ellipsisString="" layoutX="110.0" layoutY="14.0" prefHeight="53.0" prefWidth="201.0" text="Game over" textFill="WHITE" textOverrun="CENTER_WORD_ELLIPSIS" wrapText="true">
            <font>
                <Font name="Haettenschweiler" size="48.0" />
            </font>
        </Label>
        <Button fx:id="newGamePlay" layoutX="29.0" layoutY="161.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="160.0" text="New Game" textFill="#0e152e">
            <font>
                <Font name="Berlin Sans FB" size="24.0" />
            </font>
        </Button>
        <Label alignment="CENTER" contentDisplay="CENTER" ellipsisString="" layoutX="104.0" layoutY="71.0" prefHeight="34.0" prefWidth="207.0" text="Your Score" textFill="WHITE" textOverrun="CENTER_WORD_ELLIPSIS" wrapText="true">
            <font>
                <Font name="Haettenschweiler" size="31.0" />
            </font>
        </Label>
        <Button fx:id="exitApp" layoutX="227.0" layoutY="161.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="160.0" text="Exit Game" textFill="#0e152e">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
    </children>
</Pane>
